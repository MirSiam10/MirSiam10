      "cell_type": "code",
      "source": [
        "import re"
      ],
      "metadata": {
        "id": "0LQdV1Q1pZnT"
      },
      "execution_count": 966,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def clean(text):\n",
        "  text = str(text).lower()\n",
        "  text = re.sub('[.?]',' ',text)\n",
        "  text = re.sub('https?://\\S+|www.\\S+',' ', text)\n",
        "  text = re.sub('<.*?>','',text)\n",
        "  text = re.sub(r'[^\\w\\s]',' ',text)\n",
        "  text = re.sub('\\n','',text)\n",
        "  text = re.sub('\\w\\d\\w', ' ' , text)\n",
        "  text = [word for word in text.split(' ') if word in stopword]\n",
        "  text = \" \".join(text)\n",
        "  text = [stemmer. stem(word) for word in text. split(' ')]\n",
        "  text = \" \".join(text)\n",
        "  return text\n",
        "data[\"tweet\"] = data[\"tweet\"].apply(clean)\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
